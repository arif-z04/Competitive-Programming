digraph {
	n1 [label="hanoi(2, 1, 3)"]
	n2 [label="hanoi(1, 1, 2)"]
	n1 -> n2
	m2 [label="Move disk 1 from 1 to 2" shape=box]
	n2 -> m2
	m2 [label="Move disk 2 from 1 to 3" shape=box]
	n1 -> m2
	n3 [label="hanoi(1, 2, 3)"]
	n1 -> n3
	m3 [label="Move disk 1 from 2 to 3" shape=box]
	n3 -> m3
}
